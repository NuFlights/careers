          <xs:documentation>Internal code used by an carrier to identify the type of aircraft.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CarrierAircraftTypeName" type="NameType" minOccurs="0" maxOccurs="1">
