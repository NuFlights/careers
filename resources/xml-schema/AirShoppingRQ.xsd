        </xs:annotation>
      </xs:element>
      <xs:element name="TimeAfterMeasure" type="MeasureType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Time tolerance measured in hours or days.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="StationName" type="StationNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Name of the station. E.g. George Bush Intercontinental Airport</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TerminalName" type="NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name of the terminal.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BoardingGateID" type="ID_Type" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Identifier of the gate. E.g. “A5”.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeviceType">
    <xs:annotation>
      <xs:documentation>Device upon which the secure transaction will occur.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="DeviceCode" type="TouchPointDeviceCodeType" minOccurs="1" maxOccurs="1">
