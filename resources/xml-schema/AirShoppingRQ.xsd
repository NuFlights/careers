      <xs:documentation>A Customer's requested Destination arrival time and location.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IATA_LocationCode" type="IATA_LocationCodeType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>IATA defined code identifying a city or station.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Date" type="DateType" minOccurs="0" maxOccurs="1">
