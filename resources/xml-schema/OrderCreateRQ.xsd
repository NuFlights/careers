      <xs:documentation>The boarding gate identifier.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="BoardingGateID_Value"/>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="BoardingGateType">
