      <xs:documentation>Additional BDT to specify booking reference type code.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="BookingRefTypeCodeContentType"/>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="CabinColumnType">
    <xs:annotation>
      <xs:documentation>Column within a Cabin used to identify Seats and Cabin Components within a Row.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ColumnID" type="ID_Type" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifier of the column (e.g. A, B, C, etc.).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CharacteristicCode" type="SeatCharacteristicsCodeType" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>Column characteristic/property code.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinTypeType">
    <xs:annotation>
      <xs:documentation>Type of compartment of an aircraft, offering specific services (e.g. business class, economy).</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CabinTypeCode" type="CabinTypeCodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Cabin Type Code as defined by PADIS codeset (e.g. 1, 2, 3, etc.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CabinTypeName" type="NameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Name given to a cabin compartment (e.g. Business, First, Economy).</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CarrierAircraftTypeType">
    <xs:annotation>
      <xs:documentation>Carrier defined internal grouping of aircrafts with the same form, function and specification.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CarrierAircraftTypeCode" type="CodeType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Internal code used by an carrier to identify the type of aircraft.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CarrierAircraftTypeName" type="NameType" minOccurs="0">
        <xs:annotation>
          <xs:documentation>Internal name given by the carrier to this aircraft type.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CarrierType">
