          <xs:documentation>Bilaterally-agreed time limit name.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TimeLimitDateTime" type="DateTimeType">
        <xs:annotation>
          <xs:documentation>Date and time of the time limit.   Ex. 2017-05-30T09:00:00</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DescText" type="TextType">
        <xs:annotation>
          <xs:documentation>Additional text to supplement a specified time limit.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BinaryObjectType">
    <xs:annotation>
