          <xs:documentation>Booking Reference Type. Example: 6 (Passenger confirmation number) Encoding Scheme: IATA Padis Codeset - List for data element [1153] Reference Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BookingEntity" type="BookingEntityType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Airline or Organization assigning the booking information.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BookingRefTypeCodeType">
