  <xs:simpleType name="ServiceStatusCodeContentType">
    <xs:annotation>
      <xs:documentation>shows the possible statuses of a service item</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="EXPIRED">
        <xs:annotation>
          <xs:documentation>The service was delivered partially</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="IN PROGRESS">
        <xs:annotation>
          <xs:documentation>Service delivery by delivery provider is in progress.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="NOT CLAIMED">
        <xs:annotation>
          <xs:documentation>Customer did not claim the service delivery as expected by the Delivery Provider (e.g. no show).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REMOVED">
        <xs:annotation>
          <xs:documentation>Service delivery successfully completed by the Delivery Provider to the passenger on-board.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="REQUESTED">
        <xs:annotation>
          <xs:documentation>Service is ready to be ordered from the Delivery Provider. (I.e. it has not been ordered as yet!).</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="LOCKED">
        <xs:annotation>
          <xs:documentation>Order Management system blocked the Delivery Provider from Delivering the Service previously Ordered or the Service Delivery was interrupted. The delivery may resume in the future.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="DELIVERED">
        <xs:annotation>
          <xs:documentation>Service delivery successfully completed by the Delivery Provider.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="FAILED TO DELIVER">
        <xs:annotation>
          <xs:documentation>Delivery of the service failed. For example, the passenger was denied boarding.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="SUSPENDED">
        <xs:annotation>
          <xs:documentation>Delivery provider was successfully informed about the service he has to deliver.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="READY TO DELIVER">
        <xs:annotation>
          <xs:documentation>Provider is in a position to start delivery of the service.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="CONFIRMED">
        <xs:annotation>
          <xs:documentation>Delivery provider (cabin crew) is in a position to start delivery of the Service.</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="READY TO PROCEED">
        <xs:annotation>
          <xs:documentation>Airline is in a position to start delivery of the Service, or in a position to allow a Delivery Provider to prepare the service</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
