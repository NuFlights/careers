<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.iata.org/IATA/2015/00/2018.1/OrderCancelRQ" targetNamespace="http://www.iata.org/IATA/2015/00/2018.1/OrderCancelRQ" elementFormDefault="qualified" version="5.000" id="IATA2018.1">
  <xs:include schemaLocation="../2017.2/chameleon/edist_commontypes_for_AIDM.xsd"/>
  <xs:element name="OrderCancelRQ">
    <xs:annotation>
      <xs:documentation>The OrderCancel transaction set requests the cancellation of a specified order, and returns confirmation of cancellation.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PayloadAttributes" type="IATA_PayloadStandardAttributesType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>IATA standard payload attributes.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="PointOfSale" type="PointofSaleType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>Point of Sale information.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="MessageDoc" type="MessageDocType" minOccurs="0" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>NDC Message Document information.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Party" type="PartyType" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>NDC Message/ Transaction Party(s) information.  Note that information about parties is intended for business use, it is not intended for transaction authentication and routing of the message.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="Request" type="RequestType" minOccurs="1" maxOccurs="1">
          <xs:annotation>
            <xs:documentation>OrderCancelRQ request information.</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:simpleType name="AgencyTypeCodeContentType">
    <xs:annotation>
      <xs:documentation>Agency/ Agent Identification Type.
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}Identification type{/codeListName}
                {codeListIdentifier}9893{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OnlineTravelAgency"/>
      <xs:enumeration value="TravelAgency"/>
      <xs:enumeration value="TravelManagementCompany"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AgencyTypeCodeType">
    <xs:restriction base="AgencyTypeCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="AirlineDesigCodeContentType">
    <xs:annotation>
      <xs:documentation>The codelist contents of IATA Resolution 762  airline codes, or of ICAO airline codes.
                {codeListAgencyName}IATA or ICAO{/codeListAgencyName}
                {codeListName}Resolution 762  Airline Codes or ICAO Airline Codes{/codeListName}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([A-Z]{3}|[A-Z]{2})|([0-9][A-Z])|([A-Z][0-9])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="AirlineDesigCodeType">
    <xs:restriction base="AirlineDesigCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="BoardingGateID_Type">
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="8"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="BookingRefTypeCodeContentType">
    <xs:annotation>
      <xs:documentation>Booking Reference Type. Example: 6 (Passenger confirmation number)
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}Reference qualifier{/codeListName}
                {codeListIdentifier}1153{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="BookingRefTypeCodeType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="CodesetValueTextType">
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="15"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CodeType">
    <xs:restriction base="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="CountryCodeContentType">
    <xs:annotation>
      <xs:documentation>The code list of ISO 3166 two-alpha Country Codes
                {codeListAgencyName}ISO{/codeListAgencyName}
                {codeListName}3166 two-alpha Country Codes{/codeListName}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="[A-Z]{2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CountryCodeType">
    <xs:restriction base="CountryCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="CountryDialingCodeContentType">
    <xs:annotation>
      <xs:documentation>Enumeration set for the Country Dialing Code BDT.
                {codeListAgencyName}ITU{/codeListAgencyName}
                {enumerationURI}ITU-T in standards E.123 and E.164{/enumerationURI}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="CountryDialingCodeType">
    <xs:restriction base="CountryDialingCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="CountrySubDivisionCodeContentType">
    <xs:annotation>
      <xs:documentation>The list of Country Sub-Division Codes in ISO 3166-2, or referenced from ISO 3166-2 for the specific countries. Max-3-char sub-division code without preceding country code.
                {codeListAgencyName}ISO{/codeListAgencyName}
                {codeListName}3166-2 Country Subdivision Codes{/codeListName}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CountrySubDivisionCodeType">
    <xs:restriction base="CountrySubDivisionCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="CreditCardVendorCodeType">
    <xs:restriction base="xs:token">
      <xs:pattern value="[0-9a-zA-Z]{1,2}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="DateTimeType">
    <xs:restriction base="xs:dateTime"/>
  </xs:simpleType>
  <xs:simpleType name="DateType">
    <xs:restriction base="xs:date"/>
  </xs:simpleType>
  <xs:simpleType name="GenderCodeContentType">
    <xs:annotation>
      <xs:documentation>Enumeration set for Gender Code.
                {codeListAgencyName}ICAO{/codeListAgencyName}
                {codeListName}Document 9303 Sex of the holder{/codeListName}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="M">
        <xs:annotation>
          <xs:documentation>Male</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="F">
        <xs:annotation>
          <xs:documentation>Female</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="X">
        <xs:annotation>
          <xs:documentation>Unspecified</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="GenderCodeType">
    <xs:restriction base="GenderCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="IATA_LocationCodeContentType">
    <xs:annotation>
      <xs:documentation>Enumeration set for the Location Code BDT.
                {codeListAgencyName}IATA{/codeListAgencyName}
                {codeListName}Location code{/codeListName}
                {enumerationURI}http://www.iata.org/publications/Pages/code-search.aspx{/enumerationURI}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:length value="3"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="IATA_LocationCodeType">
    <xs:restriction base="IATA_LocationCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="ID_Type">
    <xs:restriction base="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="IndType">
    <xs:restriction base="xs:boolean"/>
  </xs:simpleType>
  <xs:simpleType name="MaskedCardID_Type">
    <xs:restriction base="xs:token">
      <xs:pattern value="[0-9a-zA-Z]{1,19}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="NameType">
    <xs:restriction base="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="NumberType">
    <xs:restriction base="xs:decimal"/>
  </xs:simpleType>
  <xs:simpleType name="OwnerTypeCodeContentType">
    <xs:annotation>
      <xs:documentation>Enumeration values for Owner Type BDT.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="ORA">
        <xs:annotation>
          <xs:documentation>Offer Responsible Airline</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value="POA">
        <xs:annotation>
          <xs:documentation>Participating Offer Airline</xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="OwnerTypeCodeType">
    <xs:restriction base="OwnerTypeCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="PaymentApprovalCodeCodeType">
    <xs:restriction base="PaymentApprovalCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="PaymentApprovalCodeContentType">
    <xs:annotation>
      <xs:documentation>Approval Code Method. Example: M (Manual)  Note: This is used to specify how the approval code for a credit card sale was obtained.
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}Source{/codeListName}
                {codeListIdentifier}9890{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="PaymentCardNumberType">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1,19}"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="PercentType">
    <xs:restriction base="xs:decimal"/>
  </xs:simpleType>
  <xs:simpleType name="PositionNumberType">
    <xs:restriction base="xs:double"/>
  </xs:simpleType>
  <xs:simpleType name="ProperNameType">
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="64"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SeatCharacteristicsCodeContentType">
    <xs:annotation>
      <xs:documentation>Refers to aircraft seat characteristics; seat option codes.  Examples: E (EXIT ROW), M (Middle Seat), A (Aisle seat), L (Left side of aircraft), Q (Bulk Head).  Refer to PADIS code list 9825.
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}seat option codes{/codeListName}
                {codeListIdentifier}9825{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="SeatRowCharacteristicsCodeContentType">
    <xs:annotation>
      <xs:documentation>Row characteristic. Examples: A (Extra Leg Room) B (Buffer Row) MV (Row With Movie Screen)  Encoding Scheme: IATA PADIS Code List for data element 9864 - Row Characteristic
                {codeListAgencyName}IATA{/codeListAgencyName}
                {codeListName}Row Characteristic{/codeListName}
                {codeListIdentifier}9864{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="SeatRowCharacteristicsCodeType">
    <xs:restriction base="SeatRowCharacteristicsCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="StationNameType">
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="256"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="SupplementNameType">
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="16"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TaxCodeContentType">
    <xs:annotation>
      <xs:documentation>IATA TTBS codeset
                {codeListAgencyName}IATA{/codeListAgencyName}
                {codeListName}TTBS{/codeListName}
                {enumerationURI}http://www.iata.org/services/finance/Pages/ttbs.aspx{/enumerationURI}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="TaxCodeType">
    <xs:restriction base="TaxCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="TaxQualifierCodeType">
    <xs:restriction base="xs:token"/>
  </xs:simpleType>
  <xs:simpleType name="TaxTypeCodeContentType">
    <xs:annotation>
      <xs:documentation>Applied or Exempt</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Applied"/>
      <xs:enumeration value="Exempt"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TaxTypeCodeType">
    <xs:restriction base="TaxTypeCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="TextType">
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="TouchPointDeviceCodeContentType">
    <xs:annotation>
      <xs:documentation>The device from which the request was initiated. Examples: Agent Terminal, Web Browser, Mobile Device
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}Touchpoint device type{/codeListName}
                {codeListIdentifier}TDT{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="TouchPointDeviceCodeType">
    <xs:restriction base="TouchPointDeviceCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="TouchPointEventCodeContentType">
    <xs:annotation>
      <xs:documentation>The travel event that triggered the request. Example: Order/Buy, Check-In, In Flight
                {codeListAgencyName}PADIS{/codeListAgencyName}
                {codeListName}Touchpoint transaction{/codeListName}
                {codeListIdentifier}TTT{/codeListIdentifier}</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="TouchPointEventCodeType">
    <xs:restriction base="TouchPointEventCodeContentType"/>
  </xs:simpleType>
  <xs:simpleType name="UniformResourceID_Type">
    <xs:restriction base="xs:anyURI"/>
  </xs:simpleType>
  <xs:complexType name="AggregatorType">
    <xs:annotation>
      <xs:documentation>An entity who distributes a Seller’s shopping request to multiple Airlines and aggregates subsequent responses.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AggregatorID" type="ID_Type" minOccurs="1" maxOccurs="1"/>
      <xs:element name="Name" type="ProperNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Aggregator Name.  Example: Farelogix.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="ContactInfo" type="ContactInfoType" minOccurs="0" maxOccurs="unbounded">
        <xs:annotation>
          <xs:documentation>The electronic or geographic address which a party has provided as the contact channel. For example, contact email, contact postal address, contact phone number.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AmountType">
    <xs:annotation>
      <xs:documentation>An amount is a number of monetary units specified in a currency.</xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base="xs:decimal">
        <xs:attribute name="CurCode" type="xs:token" use="optional"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="BankAccountType">
    <xs:annotation>
      <xs:documentation>Bank Account payment method definition.  Customer bank accounts for payments, either for paper checks or electronic funds transfer.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AccountID" type="ID_Type" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The bank account number or identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="AccountTypeText" type="TextType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Account Type. Examples: Checking, Savings, Investment</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BankID" type="ID_Type" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Routing Number. Example: 0260-0959-3 Note: This is the code assigned by authorities to financial institutions; sometimes called bank routing number.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CheckNumber" type="NumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Check or draft number. Example: 1234</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="OwnerName" type="ProperNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>This is the name the bank account is held under. Example: JOHN SMITH</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BoardingGateType">
    <xs:annotation>
      <xs:documentation>An exit/entry from an airport building to an aircraft. Could for example be a boarding gate, arrival gate, departing gate etc.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BoardingGateID" type="BoardingGateID_Type" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifier of the gate. E.g. “A5”.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BookingEntityType">
    <xs:annotation>
      <xs:documentation>Airline or Organization assigning the booking information.</xs:documentation>
    </xs:annotation>
    <xs:choice>
      <xs:element name="Org" type="OrgType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>An organized structure set up for a particular purpose, such as a business, government body, department, charity, or financial institution.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Carrier" type="CarrierType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Includes the carrier issuing the ticket and all carriers that carry or undertake to carry the passenger and/or his baggage or to perform any other services related to such carriage.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="BookingRefType">
    <xs:annotation>
      <xs:documentation>Information related to booking or reservation.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="BookingID" type="ID_Type" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Existing booking reference Identifier.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="TypeCode" type="BookingRefTypeCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Booking Reference Type. Example: 6 (Passenger confirmation number) Encoding Scheme: IATA Padis Codeset - List for data element [1153] Reference Qualifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="BookingEntity" type="BookingEntityType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Airline or Organization assigning the booking information.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CabinColumnType">
    <xs:annotation>
      <xs:documentation>Column within a Cabin used to identify Seats and Cabin Components within a Row.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="ColumnID" type="ID_Type" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Identifier of the column (e.g. A, B, C, etc.).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CharacteristicCode" type="SeatCharacteristicsCodeType" minOccurs="0" maxOccurs="99">
        <xs:annotation>
          <xs:documentation>Column characteristic/property code.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CarrierType">
    <xs:annotation>
      <xs:documentation>Includes the carrier issuing the ticket and all carriers that carry or undertake to carry the passenger and/or his baggage or to perform any other services related to such carriage.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AirlineDesigCode" type="AirlineDesigCodeType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Airline code assigned to a carrier. Either ICAO-defined 3-character code or IATA-defined 2-character code.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="DuplicateDesigInd" type="IndType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Indicates that the IATA Airline Designator is a duplicate code shared by multiple airlines. To identify the airline - in this case - its name must be specified in addition to the designator.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="Name" type="NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Commercial name of the carrier.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CashType">
    <xs:annotation>
      <xs:documentation>Any payment that is collected by the agent and settled between the Airline and the agent. Refer to IATA Resolution 728 Attachment A for full details.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CashInd" type="IndType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>When TRUE, payment should be made in Cash.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CheckType">
    <xs:annotation>
      <xs:documentation>Personal or bank issued order for transfer of money. Refer to IATA Resolution 728.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="Number" type="NumberType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Number found on the check which uniquely identifies the check being used as payment.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="PayeeName" type="ProperNameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name of the payee or check recipient.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="SignedDate" type="DateType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Date that the check is signed.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CityType">
    <xs:annotation>
      <xs:documentation>City (e.g., Dublin), town, or postal station (i.e., a postal service territory, often used in a military address, or metropolitan area)</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="IATA_LocationCode" type="IATA_LocationCodeType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>IATA defined code identifying a city or station.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="CityName" type="NameType" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>The name, expressed as text, of the city, town or village.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CollectionPointTaxType">
    <xs:annotation>
      <xs:documentation>Tax data breakdown by collection point for XF(Passenger Facility Fee) and ZP(federal tax segment) taxes.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="AirportAmount" type="AmountType" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Amount filed by the airport in the currency applicable to the geographical location of the airport. Example: 2789</xs:documentation>
